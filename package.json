{
  "name": "ame",
  "version": "1.0.0",
  "description": "ame app",
  "main": "src/app.js",
  "scripts": {
    "prestart": "nsp check",
    "start": "babel-node buildScripts/srcServer.js",
    "share": "lt --port 3000 --subdomain ameapp",
    "lint": "eslint .",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "cp -r src/img dist/ && cp src/CNAME dist/ && babel-node buildScripts/distServer.js",
    "deploy": "cd dist && surge .",
    "teardown": "cd dist && surge teardown",
    "serve-dist": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5aledmaged/ame.git"
  },
  "author": "Khaled Maged",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/5aledmaged/ame/issues"
  },
  "homepage": "https://github.com/5aledmaged/ame#readme",
  "dependencies": {
    "jquery": "^3.2.1",
    "raven-js": "^3.17.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "compression": "~1.7.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.0",
    "nsp": "^2.8.1",
    "open": "0.0.5",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.18.2",
    "surge": "^0.19.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-sha-hash": "^1.1.0"
  },
  "browserslist": [
    "> 1%",
    "IE > 8",
    "last 5 versions",
    "not IE < 9"
  ]
}
